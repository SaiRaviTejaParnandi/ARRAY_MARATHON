using namespace std;

int main()
{
    
    int a[7]={0,5,3,10,9,5,1};
    int sum=10;
    int count=0;
    for (int i=0;i<7;i++)
    {
        for (int j=i+1;j<7;j++)
        {
           if (a[i]+a[j]==sum)
           {
               cout << "{ "<< a[i] <<" , "<< a[j] << " } " << endl;
               count++;
           }
        }
    }
    if (count==0)
    {
        cout<<"no pair"<<endl;
    }
    
     return 0;
}
